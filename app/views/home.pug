.location-panel
  .navigation
    .col-md-4
      p All Venues
    .col-md-4
      p Music Venues
    .col-md-4
      p Theater Venues
  .grid-list
    .col-md-3(ng-repeat='item in data')
      p(data-toggle='modal' data-target='#locationModal' style='cursor: pointer') {{item.gsx$name.$t}}
      #locationModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='locationModalLabel')
        .modal-dialog(role='document')
          .modal-content
            .modal-header
              p {{item.gsx$name.$t}}
#map
